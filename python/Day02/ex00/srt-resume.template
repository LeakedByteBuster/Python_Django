<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Strict//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-strict.dtd">
<html>
<head>

	<title>{name} | {profession} | {email}</title>
	<meta http-equiv="content-type" content="text/html; charset=utf-8" />

	<meta name="keywords" content="" />
	<meta name="description" content="" />

	<link rel="stylesheet" type="text/css" href="http://yui.yahooapis.com/2.7.0/build/reset-fonts-grids/reset-fonts-grids.css" media="all" /> 
	<link rel="stylesheet" type="text/css" href="resume.css" media="all" />

</head>
<body>

<div id="doc2" class="yui-t7">
	<div id="inner">
	
		<div id="hd">
			<div class="yui-gc">
				<div class="yui-u first">
					<h1>{name}</h1>
					<h2>{profession}</h2>
				</div>

				<div class="yui-u">
					<div class="contact-info">
						<h3><a id="pdf" href="#">Download PDF</a></h3>
						<h3><a href="mailto:{email}">{email}</a></h3>
						<h3>{phone}</h3>
					</div><!--// .contact-info -->
				</div>
			</div><!--// .yui-gc -->
		</div><!--// hd -->

		<div id="bd">
			<div id="yui-main">
				<div class="yui-b">

					<div class="yui-gf">
						<div class="yui-u first">
							<h2>Profile</h2>
						</div>
						<div class="yui-u">
							<p class="enlarge">
								{profile}
							</p>
						</div>
					</div><!--// .yui-gf -->

					<div class="yui-gf">
						<div class="yui-u first">
							<h2>Skills</h2>
						</div>
						<div class="yui-u">

								<div class="talent">
									<h2>{skill1}</h2>
									<p>{skill1_desc}</p>
								</div>

								<div class="talent">
									<h2>{skill2}</h2>
									<p>{skill2_desc}</p>
								</div>

								<div class="talent">
									<h2>{skill3}</h2>
									<p>{skill3_desc}</p>
								</div>
						</div>
					</div><!--// .yui-gf -->

					<div class="yui-gf">
						<div class="yui-u first">
							<h2>Technical</h2>
						</div>
						<div class="yui-u">
							<ul class="talent">
								<li>{tech1}</li>
								<li>{tech2}</li>
								<li class="last">{tech3}</li>
							</ul>

							<ul class="talent">
								<li>{tech4}</li>
								<li>{tech5}</li>
								<li class="last">{tech6}</li>
							</ul>

							<ul class="talent">
								<li>{tech7}</li>
								<li>{tech8}</li>
								<li class="last">{tech9}</li>
							</ul>
						</div>
					</div><!--// .yui-gf-->

					<div class="yui-gf">
	
						<div class="yui-u first">
							<h2>Experience</h2>
						</div><!--// .yui-u -->

						<div class="yui-u">

							<div class="job">
								<h2>{exp1_company}</h2>
								<h3>{exp1_title}</h3>
								<h4>{exp1_dates}</h4>
								<p>{exp1_desc}</p>
							</div>

							<div class="job">
								<h2>{exp2_company}</h2>
								<h3>{exp2_title}</h3>
								<h4>{exp2_dates}</h4>
								<p>{exp2_desc}</p>
							</div>

							<div class="job">
								<h2>{exp3_company}</h2>
								<h3>{exp3_title}</h3>
								<h4>{exp3_dates}</h4>
								<p>{exp3_desc}</p>
							</div>


							<div class="job last">
								<h2>{exp4_company}</h2>
								<h3>{exp4_title}</h3>
								<h4>{exp4_dates}</h4>
								<p>{exp4_desc}</p>
							</div>

						</div><!--// .yui-u -->
					</div><!--// .yui-gf -->


					<div class="yui-gf last">
						<div class="yui-u first">
							<h2>Education</h2>
						</div>
						<div class="yui-u">
							<h2>{edu_institution}</h2>
							<h3>{edu_degree} &mdash; <strong>{edu_gpa}</strong> </h3>
						</div>
					</div><!--// .yui-gf -->


				</div><!--// .yui-b -->
			</div><!--// yui-main -->
		</div><!--// bd -->

		<div id="ft">
			<p>{name} &mdash; <a href="mailto:{email}">{email}</a> &mdash; {phone}</p>
		</div><!--// footer -->

	</div><!-- // inner -->


</div><!--// doc -->


</body>
</html>
